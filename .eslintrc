{
  "env": {
    "browser": true,
    "amd": true,
    "node": true
  },
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["prettier"],
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "rules": {
    "no-unused-vars": 2,
    "no-console": "off",
    "prettier/prettier": 2
  }
}